{
  "proxy": {
    "correlationIdHeader": "x-request-id",
    "upstream": {
      "request": {
        "httpVersion": "3.0",
        "timeoutInMilliseconds": 5200,
        "headers": {
          "proxyName": "some-proxy-name",
          "defaultHost": "this-machine-name",
          "allowHeadersWithEmptyValue": true,
          "allowHeadersWithUnderscoreInName": true,
          "ignoreAllDownstreamHeaders": true,
          "includeExternalAddress": true,
          "ignoreForwardedFor": true,
          "ignoreForwardedProtocol": true,
          "ignoreForwardedHost": true,
          "ignoreCorrelationId": true,
          "ignoreCallId": true,
          "headers": [
            {
              "name": "x-xtra-1",
              "values": [ "value1_1", "value1_2" ]
            },
            {
              "name": "x-xtra-2",
              "values": [ "value2_1", "value2_2" ]
            }
          ]
        }
      }
    },
    "downstream": {
      "response": {
        "headers": {
          "allowHeadersWithEmptyValue": true,
          "allowHeadersWithUnderscoreInName": true,
          "ignoreAllUpstreamHeaders": true,
          "includeCorrelationId": true,
          "includeCallId": true,
          "headers": [
            {
              "name": "x-xtra-1",
              "values": [ "value1_1", "value1_2" ]
            },
            {
              "name": "x-xtra-2",
              "values": [ "value2_1", "value2_2" ]
            }
          ]
        }
      }
    }
  }
}
