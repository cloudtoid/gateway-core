{
  "proxy": {
    "correlationIdHeader": "CorrelationIdHeader:$server_name",
    "upstream": {
      "request": {
        "timeoutInMilliseconds": "5200",
        "headers": {
          "proxyName": "ProxyName:$server_name",
          "defaultHost": "DefaultHost:$server_name",
          "headers": [
            {
              "name": "x-xtra-1",
              "values": [ "x-xtra-1:v1:$server_name", "x-xtra-1:v2:$server_name" ]
            },
            {
              "name": "x-xtra-2",
              "values": [ "x-xtra-2:v1:$server_name", "x-xtra-2:v2:$server_name" ]
            }
          ]
        }
      }
    },
    "downstream": {
      "response": {
        "headers": {
          "headers": [
            {
              "name": "x-xtra-1",
              "values": [ "x-xtra-1:v1:$server_name", "x-xtra-1:v2:$server_name" ]
            },
            {
              "name": "x-xtra-2",
              "values": [ "x-xtra-2:v1:$server_name", "x-xtra-2:v2:$server_name" ]
            }
          ]
        }
      }
    }
  }
}
